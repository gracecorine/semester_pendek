<%- include('../partials/header', {title : 'Status Students'}) %>

    <body>
        <h1>List Status True Students</h1>
        <a href="/baaks/students">Students</a> || <a href="/baaks/add/students">Add Students</a> || <a href="/baaks/students/status">Student Status</a> || <a href="/baaks/teachers">Teachers</a> || <a href="/baaks/courses">Courses</a>
        <hr>
        <table border="1px">
            <tr>
                <th>Id</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>NPM</th>
                <th>Email</th>
                <th>Major</th>
                <th>Class</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
            <% data.forEach(el => { %>
                <tr>
                    <td>
                        <%= el.id %>
                    </td>
                    <td>
                        <%= el.Student.first_name %>
                    </td>
                    <td>
                        <%= el.Student.last_name %>
                    </td>
                    <td>
                        <%= el.Student.npm %>
                    </td>
                    <td>
                        <%= el.Student.email %>
                    </td>
                    <td>
                        <%= el.Student.major %>
                    </td>
                    <td>
                        <%= el.Student.class %>
                    </td>
                    <td>
                        <%= el.status %>
                    </td>
                    <td>
                        <a href="/baaks/students/status/true/<%= el.id %>/delete/" onclick="return confirm('Are you sure you want to delete this Student?');">Delete</a>
                    </td>
                </tr>
                <% }) %>
        </table>
        <hr>
    </body>
    <%- include('../partials/footer') %>