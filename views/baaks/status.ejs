<%- include('../partials/header', {title : 'Students'}) %>

    <body>
        <h1>List Change Status</h1>
        <a href="/baaks/students">Students</a> || <a href="/baaks/add/students">Add Students</a> || <a href="/baaks/students/status/true">Delete Status</a> ||<a href="/baaks/students/status">Student Status</a> || <a href="/baaks/teachers">Teachers</a>
        <hr>
        <table border="1px">
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>NPM</th>
                <th>Email</th>
                <th>Major</th>
                <th>Class</th>
                <th>Status</th>
                <th>Action</th>
            </tr>
            <% data.forEach(el => { %>
                <tr>
                    <td>
                        <%= el.Student.id %>
                    </td>
                    <td>
                        <%= el.Student.fullName %>
                    </td>
                    <td>
                        <%= el.Student.npm %>
                    </td>
                    <td>
                        <%= el.Student.email %>
                    </td>
                    <td>
                        <%= el.Student.major %>
                    </td>
                    <td>
                        <%= el.Student.class %>
                    </td>
                    <td>
                        <%= el.status %>
                    </td>
                    <td>
                        <a href="/baaks/<%= el.StudentId %>/status">Change Status</a>
                    </td>
                </tr>
                <% }) %>
        </table>
        <hr>
    </body>
    <%- include('../partials/footer') %>